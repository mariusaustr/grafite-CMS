{
    "name": "grafite/cms",
    "description": "Add a CMS to any Laravel app/website to gain control of: pages, blogs, galleries, events, custom modules, images and more.",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Lantz",
            "email": "mattlantz@gmail.com"
        }
    ],

    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.82",
        "grafite/forms": "^4.0",
        "laravelcollective/html": "^6.0",
        "devfactory/minify": "^1.0",
        "aws/aws-sdk-php": "^3.18",
        "league/flysystem-aws-s3-v3": "^1.0",
        "graham-campbell/markdown": "^13.1",
        "intervention/image": "^2.4",
        "stichoza/google-translate-php": "^4.1",
        "donatj/phpuseragentparser": "^1.5"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "mikey179/vfsstream": "^1.6",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Grafite\\Cms\\": "src/"
        },
        "files": [
          "src/Helpers/blade.php",
          "src/Helpers/general.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/factories/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Grafite\\Cms\\GrafiteCmsProvider"
            ]
        }
    }
}
